<h1>Postings</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Post Type</th>
    <th>Detail</th>
    <th>Post date</th>
    <th>Cal date</th>
    <th>Exp date</th>
    <th>User post</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.post_type %></td>
    <td><%= post.detail %></td>
    <td><%= post.post_date %></td>
    <td><%= post.event_date %></td>
    <td><%= post.exp_date %></td>
    <td><%= post.user_post_id %></td>
    <td><%= post.user_id %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
